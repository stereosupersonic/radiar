%h1 Edit Tracks Info

= simple_form_for [:admin, @track_info] do |f|
  = render 'shared/error_messages', object: f.object
  = f.input :name
  = f.input :album
  = f.input :artist_name
  = f.input :year
  = f.input :youtube_id
  = f.input :pic_url
  = f.input :slug, disabled: true, hint: 'You cannot change the slug.'
  = f.input :wikidata_id
  = f.input :mbid
  = f.input :spotify_id
  = f.input :album_mbid
  = f.input :album_spotify_id
  = f.input :album_wikidata_id
  = f.input :artist_spotify_id
  = f.input :artist_wikidata_id
  = f.input :artist_mbid
  = f.input :tags, disabled: true
  = f.input :ignored, disabled: true
  .row
    = submit_button
    = back_button root_path
%br
